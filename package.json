{
  "name": "doc-detective-docs",
  "version": "2.0.5",
  "scripts": {
    "docusaurus": "concurrently \"docusaurus\" \"npm run tailwind\"",
    "start": "docusaurus start",
    "build": "npm run build:css && docusaurus build",
    "build:css": "tailwindcss -i ./src/css/tailwind.css -o ./src/css/styles.css --minify",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "npm run build",
    "build-schemas": "node ./.scripts/buildSchemaReferences.js",
    "doc-detective": "npx doc-detective@latest runTests",
    "tailwind": "tailwindcss -i ./src/css/tailwind.css -o ./src/css/styles.css --watch"
  },
  "dependencies": {
    "@docusaurus/core": "3.6.1",
    "@docusaurus/preset-classic": "3.6.1",
